                <TextOffset x="4" y="3" />
            </EditBox>
            
            <EditBox name="Shinies_EditBox_DefaultFrame" font="font_chat_text" background="EA_FullResizeImage_TanBorder" scrolling="none" maxChars="64" align="center">
