            <EditBox name="Shinies_Default_Editbox_ClearLargeBold" 		inherits="EA_EditBox_DefaultFrame" 		font="font_clear_large_bold" 		warnOnTextCropped="false"/>
            
            <EditBox name="Shinies_Default_Editbox_DefaultLarge" 		inherits="EA_EditBox_DefaultFrame" 		font="font_default_text_large" 		warnOnTextCropped="false"/>
            <EditBox name="Shinies_Default_Editbox_DefaultHuge" 		inherits="EA_EditBox_DefaultFrame" 		font="font_default_text_huge" 		warnOnTextCropped="false"/>
            <EditBox name="Shinies_Default_Editbox_DefaultGiant" 		inherits="EA_EditBox_DefaultFrame" 		font="font_default_text_giant" 		warnOnTextCropped="false"/>
        
		    <Button name="Shinies_Default_Button_ClearSmallFont" 		inherits="EA_Button_DefaultResizeable" 	font="font_clear_small" 			warnOnTextCropped="false"/>
		    <Button name="Shinies_Default_Button_ClearMediumFont" 		inherits="EA_Button_DefaultResizeable" 	font="font_clear_medium" 			warnOnTextCropped="false"/>
		    <Button name="Shinies_Default_Button_ClearMediumBoldFont" 	inherits="EA_Button_DefaultResizeable" 	font="font_clear_medium_bold" 		warnOnTextCropped="false"/>
		    <Button name="Shinies_Default_Button_ClearLargeBoldFont" 	inherits="EA_Button_DefaultResizeable" 	font="font_clear_large_bold" 		warnOnTextCropped="false"/>
		
			
			<Button name="Shinies_Button_BottomTab" inherits="EA_Button_BottomTab" font="font_clear_medium"/>
		
		   	<Button name="Shinies_ComboBox_DefaultResizableComboBoxSelectedLarge" backgroundtexture="shared_01" handleinput="false" highlighttexture="shared_01" font="font_chat_text" textalign="left"
                overlaytexture="shared_01" overlayhighlighttexture="shared_01">
                <Size>
                    <AbsPoint x="250" y="28" />
                </Size>
                <TextColors>
                    <Normal r="255" g="255" b="255" a="255" />
                    <NormalHighlit r="250" g="213" b="63" a="255" />
