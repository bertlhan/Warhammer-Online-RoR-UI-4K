            <ComboBox name="Shinies_ComboBox_DefaultResizableLarge" selectedbutton="Shinies_ComboBox_DefaultResizableComboBoxSelectedLarge"
                menubackground="EA_Window_ComboBoxMenuBackground" menuitembutton="Shinies_Button_DefaultMenuButtonLarge"
                scrollbar="EA_ScrollBar_DefaultVerticalChain" maxvisibleitems="8" >
                <Size>
                    <AbsPoint x="250" y="28" />
                </Size>
                <MenuButtonOffset x="5" y="5" />
            </ComboBox>
            
            
            <Window name="Shinies_CheckButtonWithLabel">
                <Size>
                    <AbsPoint x="280" y="32" />
                </Size>
                <EventHandlers>
                    <EventHandler event="OnInitialize" function="EA_LabelCheckButton.Initialize" />
                    <EventHandler event="OnLButtonUp" function="EA_LabelCheckButton.Toggle" />
                </EventHandlers>
                <Windows>
                    <Button name="$parentButton" inherits="EA_Button_DefaultCheckBox" handleinput="false">
                        <Anchors>
                            <Anchor point="left" relativePoint="left">
                                <AbsPoint x="0" y="0"/>
                            </Anchor>
                        </Anchors>
                    </Button>
                    <Label name="$parentLabel" inherits="Shinies_Default_Label_ClearMediumFont" autoresize="true" autoresizewidth="true" handleinput="false" warnOnTextCropped="false">
                        <Size>
                            <AbsPoint x="255" y="16" />
                        </Size>
                        <Anchors>
                            <Anchor point="right" relativePoint="left" relativeTo="$parentButton">
                                <AbsPoint x="3" y="0" />
                            </Anchor>
                        </Anchors>
                        <Color r="255" g="255" b="255" a="255" />
                    </Label>
