            <Window name="Shinies_GoldCoin" skipinput="true">
                <Size>
                    <AbsPoint x="19" y="19" />
                </Size>
                <Windows>
                    <DynamicImage name="$parentImage" inherits="EA_Templates_GoldCoin">
                        <Anchors>
                            <Anchor point="center" relativePoint="center" relativeTo="$parent"/>
                        </Anchors>
                    </DynamicImage>
                </Windows>
            </Window>
            
            <Window name="Shinies_SilverCoin" skipinput="true">
                <Size>
                    <AbsPoint x="19" y="19" />
                </Size>
                <Windows>
                    <DynamicImage name="$parentImage" inherits="EA_Templates_SilverCoin">
                        <Anchors>
                            <Anchor point="center" relativePoint="center" relativeTo="$parent"/>
                        </Anchors>
                    </DynamicImage>
                </Windows>
            </Window>
            
            <Window name="Shinies_BrassCoin" skipinput="true">
                <Size>
                    <AbsPoint x="19" y="19" />
                </Size>
