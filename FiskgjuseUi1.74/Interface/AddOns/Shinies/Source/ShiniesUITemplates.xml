    <Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Windows>
        	<Window name="ShiniesWindowDefault" inhserits="EA_Window_Default" savesettings="false"/>
            
            <Window name="ShiniesTooltipWindow" handleinput="false" layer="overlay" savesettings="false">
            	<Windows>
            		<Window name="$parentBackground" inherits="EA_Window_DefaultTooltipBackground">
	                    <Size>
	                        <AbsPoint x="0" y="0" />
	                    </Size>
	                    <Anchors>
	                        <Anchor point="topleft" relativePoint="topleft" />
	                        <Anchor point="bottomright" relativePoint="bottomright">
	                            <AbsPoint x="0" y="6" />
	                        </Anchor>
	                    </Anchors>
	                </Window>
	            </Windows>
	        </Window>
            
            <Window name="ShiniesTooltipDefault" handleinput="false" layer="overlay" savesettings="false"/>

            <Label name="ShiniesTooltipBaseLabel" sticky="false" font="font_chat_text" maxchars="1024" textalign="left" autoresize="true" wordwrap="false" handleinput="false">
                <Size>
                    <AbsPoint x="0" y="0" />
                </Size>
                <Color r="255" g="255" b="255" a="255" />
            </Label>
            
            <EditBox name="Shinies_GoldCoin_EditBox_DefaultFrame" font="font_chat_text" background="EA_FullResizeImage_TanBorder" scrolling="none" input="numbers" maxChars="5" align="rightcenter">
                <TextOffset x="4" y="3" />
            </EditBox>
            
            <EditBox name="Shinies_SilverCoin_EditBox_DefaultFrame" font="font_chat_text" background="EA_FullResizeImage_TanBorder" scrolling="none" input="numbers" maxChars="2" align="rightcenter">
                <TextOffset x="4" y="3" />
            </EditBox>
            
            <EditBox name="Shinies_BrassCoin_EditBox_DefaultFrame" font="font_chat_text" background="EA_FullResizeImage_TanBorder" scrolling="none" input="numbers" maxChars="2" align="rightcenter">
                <TextOffset x="4" y="3" />
            </EditBox>
            
            <EditBox name="Shinies_Level_EditBox_DefaultFrame" font="font_chat_text" background="EA_FullResizeImage_TanBorder" scrolling="none" input="numbers" maxChars="3" align="center">
                <TextOffset x="4" y="3" />
            </EditBox>
            
            <EditBox name="Shinies_EditBox_DefaultFrame_ThreeNumber" font="font_chat_text" background="EA_FullResizeImage_TanBorder" scrolling="none" input="numbers" maxChars="3" align="center">
