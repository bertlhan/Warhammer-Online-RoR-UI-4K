                    </DynamicImage>
                </Windows>
            </Window>
            
            <Button name="Shinies_IconButton" inherits="EA_Button_Default" texturescale="1.0" drawchildrenfirst="true">
	            <Size>
                    <AbsPoint x="64" y="64" />
                </Size>>
	            <Windows>
	            	<DynamicImage name="$parentIcon" handleinput="false" layer="background" >
	                    <Anchors>
	                        <Anchor point="topleft" relativePoint="topleft" />
	                        <Anchor point="bottomright" relativePoint="bottomright" />
	                    </Anchors>
	                    <TexDims x="64" y="64" />
	                </DynamicImage>
	            </Windows>
	            <TexCoords>
		            <Normal texture="EA_SquareFrame" x="0" y="0" />
	                <NormalHighlit texture="EA_SquareFrame_Highlight" x="0" y="0" />
	                <Pressed texture="EA_SquareFrame_Pressed" x="0" y="0" />
	                <Disabled texture="EA_SquareFrame" x="0" y="0" />
				</TexCoords>
	            <TextOffset x="5" />
	        </Button>
			
			<Button name="Shinies_IconButton_Overlay" inherits="EA_Button_Default" handleinput="false" movable="false" savesettings="false">
				<Size>
					<AbsPoint x="64" y="64"/>
				</Size>
				<TexCoords>
					<Normal texture="ShiniesIconBorderNormal" x="0" y="0" />
					<NormalHighlit texture="ShiniesIconBorderHighlight" x="0" y="0" />
					<Pressed texture="ShiniesIconBorderHighlight" x="0" y="0" />
					<Disabled texture="ShiniesIconBorderNormal" x="0" y="0" />
				</TexCoords>
			</Button>
			
			<Button name="Shinies_Button_ListSort" font="font_clear_medium"  textalign="center">
				<Size>
					<AbsPoint x="125" y="31" />
				</Size>
				<ResizeImages>
				    <Normal def="EA_Button_ListSortNormal" />
				    <NormalHighlit def="EA_Button_ListSortHighlight" />
				    <Pressed def="EA_Button_ListSortPressed" />
				    <Disabled def="EA_Button_ListSortNormal" />
				</ResizeImages>
				<TextColors>
				    <Normal             r="255" g="204" b="102" />
				    <NormalHighlit      r="255" g="85" b="0" />
				    <Pressed            r="255" g="85" b="0" />
				    <PressedHighlit     r="255" g="85" b="0"/>
				    <Disabled           r="255" g="204" b="102" />
				</TextColors>
				<TextOffset x="10" y="5" />
        	</Button>
        	
        	<Button name="Shinies_Button_DefaultListSort" inherits="Shinies_Button_ListSort" >
	            <Size>
	                <AbsPoint x="150" y="30" />
	            </Size>
	            <Windows>
	              <DynamicImage name="$parentUpArrow" inherits="EA_ListSortUpArrow">
	                  <Anchors>
	                      <Anchor point="right" relativePoint="right">
	                          <AbsPoint x="-5" y="0"/>
	                      </Anchor>
	                  </Anchors>
	              </DynamicImage>
	              <DynamicImage name="$parentDownArrow" inherits="EA_ListSortDownArrow">
	                  <Anchors>
	                      <Anchor point="center" relativePoint="center" relativeTo="$parentUpArrow">
	                          <AbsPoint x="0" y="0"/>
	                      </Anchor>
	                  </Anchors>
	              </DynamicImage>
	            </Windows>
	        </Button>
	        
	        <Label name="Shinies_Default_Label" font="font_default_text" maxchars="512" handleinput="false" textalign="leftcenter" autoresize="false" autoresizewidth="false">
                <Size>
                    <AbsPoint x="25" y="24" />
                </Size>
                <Color r="255" g="255" b="255" a="255" />
            </Label>
            
            <Label name="Shinies_Default_Label_ClearSmallFont" 			inherits="Shinies_Default_Label" 		font="font_clear_small" 			warnOnTextCropped="false"/>
            <Label name="Shinies_Default_Label_ClearSmallFont_WordWrap" inherits="Shinies_Default_Label" 		font="font_clear_small" 			warnOnTextCropped="false" wordwrap="true" background="EA_FullResizeImage_TanBorder"/>
		    <Label name="Shinies_Default_Label_ClearMediumFont" 		inherits="Shinies_Default_Label" 		font="font_clear_medium" 			warnOnTextCropped="false"/>
            <Label name="Shinies_Default_Label_ClearMediumBoldFont" 	inherits="Shinies_Default_Label" 		font="font_clear_medium_bold" 		warnOnTextCropped="false"/>
            <Label name="Shinies_Default_Label_ClearLargeFont" 			inherits="Shinies_Default_Label" 		font="font_clear_large" 			warnOnTextCropped="false"/>
            <Label name="Shinies_Default_Label_ClearLargeBoldFont" 		inherits="Shinies_Default_Label" 		font="font_clear_large_bold" 		warnOnTextCropped="false"/>
            
            <EditBox name="Shinies_Default_Editbox_ClearSmallBold" 		inherits="EA_EditBox_DefaultFrame" 		font="font_clear_small_bold"		warnOnTextCropped="false"/>
